FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.00325;

boundaryField
{
    frontandback
    {
        type            empty;
    }
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity 	 0.005;
        value           0.003; // check
    }
    outlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity 	 0.005;
        value           0.003; // check
    }
    airfoil
    {
        type            kqRWallFunction;
        value           uniform 0;
    }
}


